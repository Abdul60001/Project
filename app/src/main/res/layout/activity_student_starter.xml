<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentStarter">

    <TextView
        android:id="@+id/textView30"
        android:layout_width="274dp"
        android:layout_height="98dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Student, please select an action"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button31"
        android:layout_width="224dp"
        android:layout_height="60dp"
        android:layout_marginTop="112dp"
        android:text="View All Courses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/textView30" />

    <Button
        android:id="@+id/button39"
        android:layout_width="224dp"
        android:layout_height="60dp"
        android:layout_marginTop="96dp"
        android:text="Search and Enroll Course"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/button31" />
    <Button
        android:id="@+id/button46"
        android:layout_width="224dp"
        android:layout_height="60dp"
        android:layout_marginTop="86dp"
        android:text="View Enrolled  Courses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/button39" />


    <Button
        android:id="@+id/button47"
        android:layout_width="244dp"
        android:layout_height="60dp"
        android:layout_marginTop="102dp"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/button46" />

</androidx.constraintlayout.widget.ConstraintLayout>